{
  "name": "reqx-atelier",
  "version": "1.0.0",
  "description": "An API request tool, built with Electron and React.",
  "main": "main.js",
  "scripts": {
    "dev:renderer": "vite",
    "dev:electron": "NODE_ENV=development electron .",
    "dev": "concurrently -k \"npm run dev:renderer\" \"npm run dev:electron\"",
    "build:renderer": "vite build",
    "build:electron": "NODE_ENV=production electron-builder",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "msw": "msw init public --save",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "storybook test",
    "e2e": "playwright test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test-runner": "^0.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-istanbul": "^3.1.3",
    "concurrently": "^9.1.2",
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "jsdom": "^26.1.0",
    "msw": "^2.8.3",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "axios": "^1.9.0"
  }
}
