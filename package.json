{
  "name": "reqx-atelier",
  "version": "1.0.0",
  "description": "An API request tool, built with Electron and React.",
  "main": "main.js",
  "scripts": {
    "dev:renderer": "vite",
    "build:renderer": "vite build",
    "dev:electron": "NODE_ENV=development electron .",
    "dev": "concurrently -k \"npm run dev:renderer\" \"npm run dev:electron\"",
    "build:electron": "NODE_ENV=production electron-builder",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "msw": "msw init public --save",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "storybook test",
    "e2e": "playwright test",
    "build-storybook": "storybook build",
    "dev:all": "concurrently -k \"npm run dev\" \"npm run storybook\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.3",
    "@vitest/coverage-istanbul": "^3.1.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "^0.12.0",
    "jsdom": "^26.1.0",
    "msw": "^2.8.3",
    "playwright": "^1.52.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "i18next": "^25.1.3",
    "react-i18next": "^15.5.1",
    "react-icons": "^5.5.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
